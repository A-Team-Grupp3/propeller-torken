@model List<PropellerTorkenMain.Models.Database.Order>
@{
    ViewData["Title"] = "AdministratorOrders";
    Layout = "~/Views/Shared/_Adminlayout.cshtml";

}
<h1>HÄR SKA ORDRAR VISAS</h1>
<div id="currentOrders">
    <h2>Nuvarande ordrar</h2>
    <div id="currentorderlist">

        @if (Model != null)
        {

            <ul>
                @foreach (var items in Model)
                {
                    <li>
                        <span>@items.Id @items.Date</span>
                        <span><button class="btn btn-success">Skicka</button><button>Ta Bort</button></span>
                    </li>
                }
            </ul>
        }
    </div>
</div>