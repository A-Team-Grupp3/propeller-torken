@{
    ViewData["Title"] = "AdministratorLogIn";
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
}

<h1>Här loggar admin in :)</h1>

<div>
    <form method="post">
        <div>
            <!--<label for="fname">Användarnamn:</label><br />-->
            <span><i class="fa fa-user icon" aria-hidden="true"></i></span>
            <input type="text" id="fname" name="fname" placeholder="Username" required /><br />
        </div>
        <div>
            <!--<label>Lösenord:</label><br -->
            <i class="fa fa-key icon" aria-hidden="true"></i>
            <input type="password" id="pword" name="pword" placeholder="Password" required /><br />
        </div>
        <div>
            <br />
            <button type="submit" class="btn" id="loginbtn">Logga in</button>
        </div>
    </form>
</div>