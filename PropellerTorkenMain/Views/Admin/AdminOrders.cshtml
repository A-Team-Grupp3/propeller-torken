@model PropellerTorkenMain.Models.Database.PropellerDataContext
@{
    ViewData["Title"] = "AdministratorOrders";
    Layout = "~/Views/Shared/_Adminlayout.cshtml";

}
<h1>HÄR SKA ORDRAR VISAS</h1>
<div id="currentOrders">
    <h2>Nuvarande ordrar</h2>
    <div id="currentorderlist">

        @if (Model != null)
        {
            
            <ul>
                @foreach (var items in Model.Orders)
                {
                    <li>
                        <span>@items.Id @items._customer.Firstname @items._customer.Lastname</span>
                    </li>
                }
            </ul>
        }
    </div>
</div>